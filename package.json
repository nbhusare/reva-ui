{
  "name": "reva-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "format": "prettier --check \"**/*.+(js|jsx|ts|tsx|json|yml|yaml|md|css|scss)\"",
    "format:fix": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|yml|yaml|md|css|scss)\"",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@codingame/monaco-vscode-api": "1.76.6",
    "monaco-editor": "0.36.1",
    "monaco-languageclient": "5.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vscode": "npm:@codingame/monaco-vscode-api@~1.76.6",
    "vscode-languageclient": "9.0.1",
    "react-monaco-editor": "^0.52.0",
    "vscode-ws-jsonrpc": "^3.1.0",
    "@chakra-ui/react": "^2.8.2",
    "react-icons": "^5.2.1",
    "react-router-dom": "^6.23.1"
  },
  "resolutions": {
    "vscode-languageserver": "8.0.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "3.1.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
